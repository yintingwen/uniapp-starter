{
  "compilerOptions": {
    "types": [
      "@dcloudio/types",
      "mini-types"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*":["src/*"],
      "@config/*": ["src/config/*"],
      "@component/*": ["src/component/*"],
      "@api/*": ["src/api/*"],
      "@view/*": ["src/view/*"],
      "@store/*": ["src/store/*"],
      "@hook/*": ["src/hook/*"],
      "@constant/*": ["src/constant/*"],
      "@enum/*": ["src/enum"],
      "@util/*": ["src/util/*"]
    }
  }
}
